.container
  .row
    .col.s8
      .center
        %p.font_size_18= @post.updated_at.strftime('%Y/%m/%d')
        = image_tag @post.image, class: 'responsive-img circle', width: 120
      = @post.title

      %p= simple_format(@post.body)
        
    .col.s4
      %ul
        - 10.times do |i|
          %li any fonts
        
